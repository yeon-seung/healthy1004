<?xml version="1.0" encoding="UTF-8"?>
<!-- Sql Mapper -->
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.kosta.healthy.model.mapper.CrewMapper">
	<select id="findCrewListByName" parameterType="string"
		resultType="crewVO">
		select *
		from healthy_crew
		where crew_location like '%' || #{value} || '%'
	</select>
	<select id="getAllCrewList" parameterType="string"
		resultType="crewVO">
		select * from healthy_crew
	</select>
	<insert id="joinCrew" parameterType="hashmap">
		insert into healthy_crew(member_id, crew_id, join_date)
		values(#{item1}, #{item2}, sysdate)
	</insert>
</mapper>
 
 